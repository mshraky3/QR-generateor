{
  "version": 2,
  "builds": [
    {
      "src": "BACKEND/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "REACT/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BACKEND/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "REACT/dist/$1"
    }
  ],
  "functions": {
    "BACKEND/server.js": {
      "maxDuration": 30
    }
  }
}
